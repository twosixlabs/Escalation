{
    "plot_manager": "cytoscape",
    "plot_type": "graphvis",
    "data_sources": {
        "main_data_source": {"data_source_type": "miserables_edges"}
    },
    "title": "What is the co-occurrence of charaters in Les Miserables (by chapter)?",
