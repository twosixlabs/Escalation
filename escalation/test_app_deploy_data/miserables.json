        "edge_id": "miserables_edges:edge_id",
        "layout": {"name": "cose"},
        "style": [{
            "selector": "node", "style": {"label": "data(id)"}}],
        "element_style": [{
            "style_name": "width",
            "group": "edges",
            "column": "miserables_edges:count"
        }]
    }
}